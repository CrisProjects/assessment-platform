// Funciones para filtros de contenido
async function applyContentFilters() {
    
    // Obtener valores de filtros
    const coacheeFilter = document.getElementById('coacheeFilter')?.value || '';
    const viewModeFilter = document.getElementById('viewModeFilter')?.value || 'all';
    const statusFilter = document.getElementById('statusFilter')?.value || '';
    const searchQuery = document.getElementById('contentSearch')?.value?.toLowerCase().trim() || '';
    
    // Actualizar hint del modo de vista
    const viewModeHint = document.getElementById('viewModeHint');
    if (viewModeHint) {
        viewModeHint.textContent = viewModeFilter === 'unique' 
            ? 'Agrupa contenido por título (muestra estadísticas)' 
            : 'Vista detallada de todas las asignaciones';
    }
    
    // Mostrar loading
    const loadingState = document.getElementById('contentLoadingState');
    const container = document.getElementById('contentListContainer');
    if (loadingState) loadingState.style.display = 'block';
    if (container) container.innerHTML = '';
    
    try {
        // Construir URL con parámetros
        let url = `/api/coach/content?view_mode=${viewModeFilter}`;
        if (coacheeFilter) {
            url += `&coachee_id=${coacheeFilter}`;
        }
        
        
        // Fetch con credenciales
        const response = await fetch(url, {
            credentials: 'include',
            headers: {
                'Accept': 'application/json'
            }
        });
        
        if (!response.ok) {
            throw new Error(`HTTP ${response.status}: ${response.statusText}`);
        }
        
        const data = await response.json();
        
        if (!data.success) {
            throw new Error(data.error || 'Error al cargar contenido');
        }
        
        let content = data.content || [];
        
        // Aplicar filtros locales
        
        // 1. Filtrar por estado (viewed/pending)
        if (statusFilter === 'viewed') {
            content = content.filter(c => c.is_viewed);
        } else if (statusFilter === 'pending') {
            content = content.filter(c => !c.is_viewed);
        }
        
        // 2. Filtrar por búsqueda de texto
        if (searchQuery) {
            content = content.filter(c => {
                const title = (c.title || '').toLowerCase();
                const description = (c.description || '').toLowerCase();
                const coacheeName = (c.coachee_name || '').toLowerCase();
                const matchesSearch = title.includes(searchQuery) || 
                       description.includes(searchQuery) || 
                       coacheeName.includes(searchQuery);
                
                if (matchesSearch) {
                }
                
                return matchesSearch;
            });
        }
        
        
        // Actualizar contador
        const resultsCount = document.getElementById('contentResultsCount');
        if (resultsCount) {
            const label = content.length === 1 ? 'resultado' : 'resultados';
            resultsCount.textContent = `${content.length} ${label}`;
            resultsCount.className = content.length > 0 ? 'badge bg-primary' : 'badge bg-secondary';
        }
        
        // Ocultar loading
        if (loadingState) loadingState.style.display = 'none';
        
        // Obtener app de Alpine.js y renderizar
        const appElement = document.querySelector('[x-data]');
        if (appElement && appElement.__x && appElement.__x.$data) {
            const app = appElement.__x.$data;
            app.renderAssignedContent(content);
        } else {
            console.error('❌ [FILTER] No se pudo acceder a Alpine.js, renderizando directamente...');
            // Fallback: renderizar sin Alpine.js
            renderContentDirectly(content);
        }
        
    } catch (error) {
        console.error('❌ [FILTER] Error:', error);
        if (loadingState) loadingState.style.display = 'none';
        if (container) {
            container.innerHTML = `
                <div class="col-12">
                    <div class="alert alert-danger">
                        <i class="fas fa-exclamation-triangle me-2"></i>
                        Error al aplicar filtros: ${error.message}
                    </div>
                </div>
            `;
        }
    }
}

// Función fallback para renderizar sin Alpine.js
function renderContentDirectly(contentList) {
    const container = document.getElementById('contentListContainer');
    const emptyState = document.getElementById('contentEmptyState');
    
    if (!container) {
        console.error('❌ Container contentListContainer no encontrado');
        return;
    }
    
    if (!contentList || contentList.length === 0) {
        container.innerHTML = '';
        if (emptyState) emptyState.style.display = 'block';
        return;
    }
    
    if (emptyState) emptyState.style.display = 'none';
    
    // Construir HTML completo con vistas previas (igual que renderAssignedContent)
    let html = '';
    
    contentList.forEach(content => {
        const isVideo = content.content_type === 'video';
        const isDocument = content.content_type === 'document';
        
        // Detectar YouTube
        let youtubeVideoId = null;
        if (content.content_url) {
            if (content.content_url.includes('youtube.com/watch?v=')) {
                youtubeVideoId = content.content_url.split('v=')[1]?.split('&')[0];
            } else if (content.content_url.includes('youtu.be/')) {
                youtubeVideoId = content.content_url.split('youtu.be/')[1]?.split('?')[0];
            }
        }
        
        // Detectar Instagram
        let instagramVideoId = null;
        if (content.content_url) {
            const instagramPatterns = [
                /instagram\.com\/(?:p|reel|tv)\/([A-Za-z0-9_-]+)/,
                /instagram\.com\/(?:p|reel|tv)\/([A-Za-z0-9_-]+)\//
            ];
            for (const pattern of instagramPatterns) {
                const match = content.content_url.match(pattern);
                if (match && match[1]) {
                    instagramVideoId = match[1];
                    break;
                }
            }
        }
        
        // Construir HTML de vista previa
        let videoEmbedHTML = '';
        
        if (youtubeVideoId) {
            videoEmbedHTML = `
            <div class="content-preview has-embedded-video">
                <div class="ratio ratio-16x9" style="position: relative; border-radius: 8px; overflow: hidden;">
                    <iframe 
                        src="https://www.youtube.com/embed/${youtubeVideoId}?rel=0&modestbranding=1" 
                        title="${content.title || 'Video'}"
                        frameborder="0" 
                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                        allowfullscreen
                        style="border-radius: 8px; width: 100%; height: 100%;">
                    </iframe>
                </div>
            </div>
            `;
        } else if (instagramVideoId) {
            videoEmbedHTML = `
            <div class="content-preview has-embedded-video">
                <div class="ratio ratio-9x16" style="position: relative; border-radius: 8px; overflow: hidden;">
                    <iframe 
                        src="https://www.instagram.com/p/${instagramVideoId}/embed" 
                        title="${content.title || 'Video'}"
                        frameborder="0" 
                        scrolling="no" 
                        allowtransparency="true"
                        style="border-radius: 8px; width: 100%; height: 100%;">
                    </iframe>
                </div>
            </div>
            `;
        } else if (isDocument && content.content_url) {
            videoEmbedHTML = `
                <div class="content-preview">
                    <div style="width: 100%; height: 300px; background: #f5f5f5; overflow-y: auto; overflow-x: hidden; -webkit-overflow-scrolling: touch; position: relative; border-radius: 8px;">
                        <canvas id="doc-preview-assigned-${content.id}"
                                width="1200"
                                height="1000"
                                style="width: 100%; height: auto; display: block; cursor: pointer;"
                                onclick="window.open('${content.content_url}', '_blank')">
                        </canvas>
                        <div id="doc-loading-assigned-${content.id}" style="position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); text-align: center; color: #6c757d; pointer-events: none;">
                            <div class="spinner-border" role="status" style="width: 3rem; height: 3rem;">
                                <span class="visually-hidden">Cargando...</span>
                            </div>
                            <p class="mt-2">Cargando documento...</p>
                        </div>
                    </div>
                </div>
            `;
        }
        
        const viewedBadge = content.is_viewed 
            ? '<span class="badge bg-success ms-2"><i class="fas fa-check"></i> Visto</span>'
            : '<span class="badge bg-warning ms-2"><i class="fas fa-eye"></i> Pendiente</span>';
        
        html += `
            <div class="col-md-6 col-lg-4 mb-4">
                <div class="card h-100" style="border: 1px solid #e5e7eb; border-radius: 12px;">
                    ${videoEmbedHTML}
                    
                    <div class="card-body">
                        <div class="d-flex justify-content-between align-items-start mb-2">
                            <div class="flex-grow-1">
                                <h6 class="card-title mb-1">${content.title || 'Sin título'}</h6>
                                ${viewedBadge}
                            </div>
                        </div>
                        ${content.description ? `<p class="card-text small text-muted mb-2">${content.description}</p>` : ''}
                        <div class="d-flex justify-content-between align-items-center mt-3">
                            <small class="text-muted">
                                <i class="fas fa-user me-1"></i>
                                ${content.coachee_name || 'Sin asignar'}
                            </small>
                            <button class="btn btn-sm btn-outline-danger" onclick="deleteContentCoach(${content.id})">
                                <i class="fas fa-trash"></i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        `;
    });
    
    container.innerHTML = html;
    
    // Renderizar PDFs después de cargar el HTML
    setTimeout(() => {
        const documentCount = contentList.filter(c => c.content_type === 'document' && c.content_url).length;
        
        contentList.forEach(content => {
            if (content.content_type === 'document' && content.content_url) {
                const canvasId = 'doc-preview-assigned-' + content.id;
                const canvas = document.getElementById(canvasId);
                if (canvas) {
                    generateDocumentPreviewAssigned(content.content_url, canvasId);
                } else {
                    console.error(`❌ [FILTER-DIRECT] Canvas ${canvasId} NO encontrado en el DOM`);
                }
            }
        });
    }, 200);
}

function clearContentFilters() {
    
    const coacheeFilter = document.getElementById('coacheeFilter');
    const viewModeFilter = document.getElementById('viewModeFilter');
    const statusFilter = document.getElementById('statusFilter');
    const contentSearch = document.getElementById('contentSearch');
    
    if (coacheeFilter) coacheeFilter.value = '';
    if (viewModeFilter) viewModeFilter.value = 'all';
    if (statusFilter) statusFilter.value = '';
    if (contentSearch) contentSearch.value = '';
    
    // Actualizar hint
    const viewModeHint = document.getElementById('viewModeHint');
    if (viewModeHint) {
        viewModeHint.textContent = 'Vista detallada de todas las asignaciones';
    }
    
    // Recargar contenido sin filtros
    refreshContentList();
}

function refreshContentList() {
    const appElement = document.querySelector('[x-data]');
    if (appElement && appElement.__x) {
        const app = appElement.__x.$data;
        if (app && typeof app.loadAssignedContent === 'function') {
            app.loadAssignedContent();
        } else {
            console.error('❌ loadAssignedContent no está disponible');
        }
    } else {
        console.error('❌ No se encontró Alpine.js');
    }
}

// Función global para eliminar contenido (llamada desde HTML renderizado)
async function deleteContentCoach(contentId) {
    if (!confirm('¿Estás seguro de que deseas eliminar este contenido?')) {
        return;
    }
    
    try {
        const response = await fetch(`/api/coach/content/${contentId}`, {
            method: 'DELETE'
        });
        
        const data = await response.json();
        
        if (data.success) {
            showNotification('✅ Contenido eliminado correctamente', 'success');
            // Recargar la lista
            const app = document.querySelector('[x-data]')?.__x?.$data;
            if (app && app.loadAssignedContent) {
                await app.loadAssignedContent();
            }
        } else {
            showNotification('❌ ' + (data.error || 'Error al eliminar contenido'), 'error');
        }
    } catch (error) {
        console.error('Error eliminando contenido:', error);
        showNotification('❌ Error de conexión', 'error');
    }
}
</script>

