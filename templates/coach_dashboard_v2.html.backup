    // Los listeners se inicializarán cuando se cargue el módulo desde Alpine.js
    
    // Exponer el dashboard de Alpine al scope global para acceso desde event listeners
    // Esperar a que Alpine esté disponible
    if (typeof Alpine !== 'undefined' && Alpine.$data) {
        window.coachDashboard = Alpine.$data(document.body);
    } else {
        // Intentar de nuevo después de un pequeño delay
        setTimeout(() => {
            if (typeof Alpine !== 'undefined' && Alpine.$data) {
                window.coachDashboard = Alpine.$data(document.body);
            }
        }, 100);
    }
    
    // Cargar contador de solicitudes pendientes al iniciar
    async function updateSessionRequestsBadge() {
        try {
            const response = await fetch('/api/coach/session-requests');
            const data = await response.json();
            
            if (data.success) {
                const count = data.requests ? data.requests.length : 0;
                const badge = document.getElementById('sessionRequestsBadge');
                
                if (badge) {
                    if (count > 0) {
                        badge.textContent = count;
                        badge.style.display = 'inline-block';
                    } else {
                        badge.style.display = 'none';
                    }
                }
            }
        } catch (error) {
            console.error('Error al cargar contador de solicitudes:', error);
        }
    }
    
    // Actualizar badge al cargar
    updateSessionRequestsBadge();
    
    // Actualizar badge cada 2 minutos
    setInterval(updateSessionRequestsBadge, 120000);
    
    // Listener para cuando se muestre la pestaña de solicitudes
    const requestsTab = document.getElementById('requests-tab');
    if (requestsTab) {
        requestsTab.addEventListener('shown.bs.tab', function(event) {
            if (window.coachDashboard && typeof window.coachDashboard.loadSessionRequests === 'function') {
                window.coachDashboard.loadSessionRequests();
            } else {
                console.error('❌ window.coachDashboard.loadSessionRequests no disponible');
            }
        });
        // También listener para click directo - CARGAR SOLICITUDES INMEDIATAMENTE AL CLICK
        requestsTab.addEventListener('click', function(e) {
            
            // FORZAR la activación del tab-pane
            const requestsPane = document.getElementById('requests-pane');
            if (requestsPane) {
                // Ocultar todos los otros tab-panes
                document.querySelectorAll('#agendaTabs ~ .tab-content .tab-pane').forEach(pane => {
                    pane.classList.remove('show', 'active');
                });
                // Mostrar el pane de solicitudes
                requestsPane.classList.add('show', 'active');
                
                // Desactivar todos los otros tabs
                document.querySelectorAll('#agendaTabs .nav-link').forEach(tab => {
                    tab.classList.remove('active');
                });
                // Activar el tab de solicitudes
                requestsTab.classList.add('active');
            }
            
            // Dar tiempo para que Bootstrap cambie la tab antes de cargar
            setTimeout(() => {
                if (window.coachDashboard && typeof window.coachDashboard.loadSessionRequests === 'function') {
                    window.coachDashboard.loadSessionRequests();
                }
            }, 50);
        });
    } else {
        console.error('❌ No se encontró el tab "requests-tab" en el DOM');
    }
    
    // Listener para el tab de solicitudes dentro de Mi Agenda
    const solicitudesTab = document.getElementById('solicitudes-tab');
    if (solicitudesTab) {
        solicitudesTab.addEventListener('shown.bs.tab', function(event) {
            if (window.coachDashboard && typeof window.coachDashboard.loadSessionRequests === 'function') {
