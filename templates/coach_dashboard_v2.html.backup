        // Inicializar calendario
        initializeCalendar() {
            const calendarEl = document.getElementById('calendar');
            if (!calendarEl || this.calendar) return;
            
            this.calendar = new FullCalendar.Calendar(calendarEl, {
                initialView: 'dayGridMonth',
                locale: 'es',
                firstDay: 1, // Comenzar semana el lunes (0=domingo, 1=lunes)
                height: 'auto',
                headerToolbar: {
                    left: 'prev,next today',
                    center: 'title',
                    right: 'dayGridMonth,timeGridWeek,timeGridDay'
                },
                buttonText: {
                    today: 'Hoy',
                    month: 'Mes',
                    week: 'Semana',
                    day: 'Día'
                },
                events: async (fetchInfo, successCallback, failureCallback) => {
                    try {
                        const response = await fetch(`/api/coach/sessions?start_date=${fetchInfo.startStr}&end_date=${fetchInfo.endStr}`);
                        const data = await response.json();
                        
                        if (data.success) {
                            const events = data.sessions.map(session => {
                                // Diferenciar título según tipo de sesión
                                let title;
                                if (session.session_type === 'self_activity') {
                                    title = `🔒 ${session.activity_title || session.title || 'Tiempo Bloqueado'}`;
                                } else {
                                    title = `${session.coachee_name} - ${session.title || 'Sesión'}`;
                                }
                                
                                return {
                                    id: session.id,
                                    title: title,
                                    start: session.start,
                                    end: session.end,
                                    backgroundColor: this.getSessionColor(session.status, session.session_type),
                                    extendedProps: session
                                };
                            });
                            successCallback(events);
                        }
                    } catch (error) {
                        failureCallback(error);
                    }
                },
                eventClick: (info) => {
                    this.showSessionDetails(info.event);
                },
                selectable: true,
                navLinks: true
            });
            
            this.calendar.render();
            
            // Cargar próximas sesiones de la semana
            this.loadUpcomingSessions();
