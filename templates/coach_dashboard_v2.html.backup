/**
 * Elimina una pregunta del formulario
 */
function removeQuestion(questionId) {
    const questionElement = document.querySelector(`[data-question-id="${questionId}"]`);
    if (questionElement) {
        questionElement.remove();
        
        // Check if there are no more questions
        const remainingQuestions = document.querySelectorAll('.question-item');
        if (remainingQuestions.length === 0) {
            const noQuestionsMsg = document.getElementById('noQuestionsMessage');
            if (noQuestionsMsg) {
                noQuestionsMsg.style.display = 'block';
            }
        }
    }
}

/**
 * Valida el formulario de creación de evaluación
 */
function validateAssessmentForm() {
    const title = document.getElementById('assessmentTitle').value.trim();
    const category = document.getElementById('assessmentCategory').value;
    const questions = document.querySelectorAll('.question-item');
    
    if (!title) {
        showToast('Por favor ingresa un título para la evaluación', 'error');
        return false;
    }
    
    if (!category) {
        showToast('Por favor selecciona una categoría', 'error');
        return false;
    }
    
    if (questions.length === 0) {
        showToast('Debes agregar al menos una pregunta', 'error');
        return false;
    }
    
    // Validate each question has text
    let allQuestionsValid = true;
    questions.forEach((questionCard, index) => {
        const questionText = questionCard.querySelector('.question-text').value.trim();
        if (!questionText) {
            showToast(`La pregunta #${index + 1} necesita un texto`, 'error');
            allQuestionsValid = false;
        }
    });
    
    return allQuestionsValid;
}

/**
 * Recopila los datos del formulario
 */
function collectAssessmentData(status = 'draft') {
    const title = document.getElementById('assessmentTitle').value.trim();
    const description = document.getElementById('assessmentDescription').value.trim();
    const category = document.getElementById('assessmentCategory').value;
    
    const questions = [];
    const questionCards = document.querySelectorAll('.question-item');
    
    questionCards.forEach((card, index) => {
        const text = card.querySelector('.question-text').value.trim();
        const dimension = card.querySelector('.question-dimension').value.trim();
        const type = card.querySelector('.question-type').value;
        
        questions.push({
            text: text,
            dimension: dimension || null,
            question_type: type,
            order: index + 1
        });
    });
    
    return {
        title: title,
        description: description || null,
        category: category,
        status: status,
        questions: questions
    };
}

/**
 * Guarda la evaluación como borrador o la publica
 */
async function saveAssessmentDraft(publish = false) {
    
    if (!validateAssessmentForm()) {
        return;
    }
    
    // Si estamos editando, usar endpoint de actualización
    if (editingAssessmentId && !publish) {
        await updateExistingAssessment();
        return;
    }
    
    const status = publish ? 'published' : 'draft';
    const data = collectAssessmentData(status);
    
    
    try {
        const response = await fetch('/api/coach/assessments/create', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify(data)
        });
        
        const result = await response.json();
        
        if (response.ok) {
            const message = publish 
                ? 'Evaluación publicada exitosamente. Ya está disponible para asignar a coachees.'
                : 'Borrador guardado exitosamente. Puedes publicarlo cuando esté listo.';
            showToast(message, 'success');
            
            // Reset form
            resetAssessmentForm();
            
            // Reload drafts if saved as draft
            if (!publish) {
                loadDraftAssessments();
                // Switch to drafts tab
                const borradoresTab = document.getElementById('borradores-eval-tab');
                if (borradoresTab) {
                    borradoresTab.click();
                }
            }
            
        } else {
            throw new Error(result.error || 'Error al guardar la evaluación');
        }
        
    } catch (error) {
        console.error(`❌ ${publish ? 'PUBLISH' : 'SAVE-DRAFT'}-ASSESSMENT: Error:`, error);
        showToast('Error al guardar: ' + error.message, 'error');
    }
}

/**
