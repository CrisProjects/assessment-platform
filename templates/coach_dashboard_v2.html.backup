        selectedCoacheeForEdit: null,
        currentNotesCoacheeId: null,
        currentNotesCoacheeName: '',
        
        // Calendario
        calendar: null,
        
        // Fecha/Hora actual
        currentDate: '',
        currentTime: '',
        
        // Inicialización
        async init() {
            console.log('🚀 [DASHBOARD] Iniciando dashboard...');
            this.createParticles();
            this.updateDateTime();
            setInterval(() => this.updateDateTime(), 1000);
            
            // Ocultar todos los módulos al inicio para evitar parpadeo
            document.querySelectorAll('.module-content').forEach(module => {
                module.style.display = 'none';
            });
            
            console.log('📥 [DASHBOARD] Cargando perfil...');
            await this.loadProfile();
            console.log('📊 [DASHBOARD] Cargando estadísticas...');
            await this.loadStats();
            
            // Recuperar la última sección activa desde localStorage
            const lastActiveModule = localStorage.getItem('coach_active_module') || 'overview';
            console.log(`📂 [DASHBOARD] Mostrando módulo: ${lastActiveModule}`);
            await this.showModule(lastActiveModule);
            console.log('✅ [DASHBOARD] Inicialización completa');
        },
        
        // Crear partículas animadas
        createParticles() {
            const particlesContainer = document.querySelector('.particles');
            if (!particlesContainer) return;
            
            for (let i = 0; i < 30; i++) {
                const particle = document.createElement('div');
                particle.className = 'particle';
                particle.style.left = Math.random() * 100 + '%';
                particle.style.animationDelay = Math.random() * 20 + 's';
                particle.style.width = particle.style.height = (Math.random() * 4 + 2) + 'px';
                particle.style.animationDuration = (Math.random() * 10 + 20) + 's';
                particlesContainer.appendChild(particle);
            }
        },
        
        // Actualizar fecha y hora
        updateDateTime() {
            const now = new Date();
            const dateOptions = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };
            const timeOptions = { hour: '2-digit', minute: '2-digit', second: '2-digit' };
            this.currentDate = now.toLocaleDateString('es-ES', dateOptions);
            this.currentTime = now.toLocaleTimeString('es-ES', timeOptions);
        },
        
        // Verificar si la sesión expiró y redirigir
        checkSessionExpired(data) {
            if (data && data.session_expired && data.redirect_url) {
                window.location.href = data.redirect_url;
                return true;
            }
            return false;
        },
        
        // Cargar perfil del coach
        async loadProfile() {
            try {
                const response = await fetch('/api/coach/profile');
                const data = await response.json();
                
                // Verificar sesión expirada
                if (this.checkSessionExpired(data)) return;
                
                if (data.success) {
                    const profile = data.profile;
                    this.coachName = profile.full_name || profile.username || 'Coach';
                    
                    if (profile.avatar_url) {
                        this.coachAvatar = `<img src="${profile.avatar_url}" alt="${this.coachName}" style="width: 100%; height: 100%; object-fit: cover; border-radius: 50%;">`;
                    } else {
                        const initial = this.coachName.charAt(0).toUpperCase();
                        this.coachAvatar = initial;
                    }
                }
            } catch (error) {
                console.error('❌ Error loading coach profile:', error);
            }
        },
        
        // Cargar estadísticas
        async loadStats() {
            this.loading.stats = true;
            try {
                const response = await fetch('/api/coach/my-coachees');
                const data = await response.json();
                
                // Verificar sesión expirada
                if (this.checkSessionExpired(data)) return;
                
                if (data.success) {
                    this.stats.totalCoachees = data.coachees.length;
                    
                    let totalEvaluations = 0;
                    let totalScore = 0;
                    let evaluationsWithScore = 0;
                    let completedEvaluations = 0;
                    let totalProgress = 0;
                    
                    data.coachees.forEach(coachee => {
                        if (coachee.evaluations_count) {
                            totalEvaluations += coachee.evaluations_count;
                            completedEvaluations += coachee.evaluations_count;
                        }
                        if (coachee.avg_score) {
                            totalScore += coachee.avg_score * coachee.evaluations_count;
                            evaluationsWithScore += coachee.evaluations_count;
                            totalProgress += coachee.avg_score;
                        }
                    });
                    
                    this.stats.completedAssessments = totalEvaluations;
                    this.stats.completedEvaluations = completedEvaluations;
                    const assignedTasks = data.assigned_evaluation_tasks || 0;
                    this.stats.pendingAssessments = Math.max(0, assignedTasks - totalEvaluations);
                    
                    const avgScore = evaluationsWithScore > 0 ? Math.round(totalScore / evaluationsWithScore) : 0;
                    this.stats.avgScore = avgScore + '%';
                    
                    // Calcular progreso promedio
                    this.stats.averageProgress = evaluationsWithScore > 0 ? Math.round(totalProgress / evaluationsWithScore) : 0;
                    
                    // Simular sesiones este mes (esto debería venir de la API en el futuro)
                    this.stats.sessionsThisMonth = Math.floor(Math.random() * 20) + 5; // Placeholder
                }
            } catch (error) {
                console.error('❌ Error loading stats:', error);
            } finally {
                this.loading.stats = false;
            }
        },
        
        // Mostrar módulo
        async showModule(moduleId, shouldScroll = false) {
            this.activeModule = moduleId;
            
            // Guardar la sección activa en localStorage
            localStorage.setItem('coach_active_module', moduleId);
            
            // Ocultar todos los módulos
            document.querySelectorAll('.module-content').forEach(module => {
                module.style.display = 'none';
            });
            
            // Remover clase active de todos los enlaces
            document.querySelectorAll('[data-module]').forEach(link => {
                link.classList.remove('active');
            });
            
            // Mostrar módulo seleccionado
            const selectedModule = document.getElementById(moduleId);
            if (selectedModule) {
                selectedModule.style.display = 'block';
                selectedModule.classList.add('module-visible');
                
                // Scroll suave al módulo SOLO si se solicita explícitamente (click del usuario)
                if (shouldScroll) {
                    setTimeout(() => {
                        const isMobileOrTablet = window.innerWidth <= 1024;
                        if (isMobileOrTablet) {
                            selectedModule.scrollIntoView({
                                behavior: 'smooth',
                                block: 'start'
                            });
                        }
                    }, 100);
                }
            }
            
            // Agregar clase active al enlace seleccionado
            const selectedLink = document.querySelector(`[data-module="${moduleId}"]`);
            if (selectedLink) {
                selectedLink.classList.add('active');
            }
            
            // Cargar datos específicos del módulo
            switch(moduleId) {
                case 'overview':
                    await this.loadStats();
                    setTimeout(() => {
                        this.initializeOverviewCharts();
                    }, 100);
                    break;
                case 'mis-coachees':
                    await this.loadCoachees();
                    break;
                case 'gestion-tareas':
                    await this.loadTasks();
                    break;
                case 'seleccionar-evaluaciones':
                    await this.loadAvailableAssessments();
                    break;
                case 'planes-desarrollo':
                    await this.loadDevelopmentPlanRequests();
                    break;
                case 'evaluaciones-pendientes':
                    await this.loadPendingEvaluations();
                    break;
                case 'invitar-coachee':
                    await this.loadAssessmentsForInvitation();
                    break;
                case 'agregar-contenido':
                    // Resetear flag de inicialización de video listeners
                    if (typeof videoListenersInitialized !== 'undefined') {
                        videoListenersInitialized = false;
                    }
                    await this.loadAssignedContent();
                    break;
                case 'contenido-prueba':
                    // Inicializar calendario (tab activo por defecto ahora es Calendario)
                    setTimeout(() => {
                        this.initializeCalendar();
                        this.loadMyAvailability();
                        // Cargar coachees para el formulario de citas directas
                        loadCoacheesForDirectAppointment();
                    }, 100);
                    // Cargar solicitudes en segundo plano
                    setTimeout(() => {
                        this.loadSessionRequests();
                    }, 200);
                    break;
                case 'solicitudes-sesion':
                    await this.loadSessionRequests();
                    break;
                case 'gestion':
                    // Cargar lista de coachees para el selector de citas directas
                    setTimeout(() => {
                        loadCoacheesForDirectAppointment();
                    }, 100);
                    break;
            }
        },
        
