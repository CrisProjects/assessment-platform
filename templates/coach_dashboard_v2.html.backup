                                <input type="password" class="form-control" id="newCoacheePassword" required
                                    placeholder="Contraseña para el coachee">
                            </div>
                            
                            <div class="mb-3">
                                <label for="newEvaluationMessage" class="form-label">Mensaje personalizado (opcional):</label>
                                <textarea class="form-control" id="newEvaluationMessage" rows="3" 
                                    placeholder="Mensaje para el coachee..."></textarea>
                            </div>
                            
                            <div class="mb-3">
                                <label for="newEvaluationDueDate" class="form-label">Fecha límite (opcional):</label>
                                <input type="date" class="form-control" id="newEvaluationDueDate">
                            </div>
                            
                            <button type="button" class="btn btn-success" onclick="createCoacheeAndAssignEvaluation(${assessmentId})">
                                <i class="fas fa-user-plus me-1"></i>Crear Coachee y Asignar
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    `;
    
    // Remover modal previo si existe
    const existingModal = document.getElementById('assignEvaluationToCoacheeModal');
    if (existingModal) {
        existingModal.remove();
    }
    
    // Añadir nuevo modal al DOM
    document.body.appendChild(assignEvaluationModal);
    
    // Mostrar modal
    const modal = new bootstrap.Modal(assignEvaluationModal);
    modal.show();
    
    // Cargar coachees en el select
    loadCoacheesForAssignment();
    
    // Limpiar modal cuando se cierre
    assignEvaluationModal.addEventListener('hidden.bs.modal', function () {
        assignEvaluationModal.remove();
    });
}

// Función helper para mostrar notificaciones
function showNotification(message, type = 'info') {
    const toastContainer = document.getElementById('toastContainer') || createToastContainer();
    const toast = document.createElement('div');
    toast.className = `toast align-items-center text-white bg-${type === 'error' ? 'danger' : type} border-0`;
    toast.setAttribute('role', 'alert');
    toast.innerHTML = `
        <div class="d-flex">
            <div class="toast-body">${message}</div>
            <button type="button" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast"></button>
        </div>
    `;
    toastContainer.appendChild(toast);
    const bsToast = new bootstrap.Toast(toast);
    bsToast.show();
    setTimeout(() => toast.remove(), 5000);
}

function createToastContainer() {
    const container = document.createElement('div');
    container.id = 'toastContainer';
    container.className = 'toast-container position-fixed top-0 end-0 p-3';
    container.style.zIndex = '9999';
    document.body.appendChild(container);
    return container;
}

// Cargar coachees para asignación
async function loadCoacheesForAssignment() {
    try {
        const response = await fetch('/api/coach/my-coachees');
        const data = await response.json();
        
        const select = document.getElementById('existingCoacheeSelect');
        if (!select) return;
        
        if (data.success && data.coachees && data.coachees.length > 0) {
            select.innerHTML = '<option value="">Seleccionar coachee...</option>' +
                data.coachees.map(c => `<option value="${c.id}">${c.name} (${c.email})</option>`).join('');
        } else {
            select.innerHTML = '<option value="">No hay coachees disponibles</option>';
        }
    } catch (error) {
        console.error('Error loading coachees:', error);
        const select = document.getElementById('existingCoacheeSelect');
        if (select) {
            select.innerHTML = '<option value="">Error al cargar coachees</option>';
        }
    }
}

// Asignar evaluación a coachee existente
async function assignEvaluationToExistingCoachee(assessmentId) {
    const coacheeId = document.getElementById('existingCoacheeSelect').value;
    const message = document.getElementById('existingEvaluationMessage').value;
    const dueDate = document.getElementById('existingEvaluationDueDate').value;
    
    if (!coacheeId) {
        showNotification('Por favor selecciona un coachee', 'error');
        return;
    }
    
    try {
        const response = await fetch('/api/coach/assign-evaluation', {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({
                assessment_id: assessmentId,
                coachee_id: coacheeId,
                message: message,
                due_date: dueDate
            })
        });
        
        const data = await response.json();
        
        if (data.success) {
            showNotification('Evaluación asignada exitosamente', 'success');
            const modal = bootstrap.Modal.getInstance(document.getElementById('assignEvaluationToCoacheeModal'));
            modal.hide();
        } else {
            showNotification(data.error || 'Error al asignar evaluación', 'error');
        }
    } catch (error) {
        console.error('Error:', error);
        showNotification('Error al asignar evaluación', 'error');
    }
}

// Crear coachee y asignar evaluación
async function createCoacheeAndAssignEvaluation(assessmentId) {
    const name = document.getElementById('newCoacheeName').value;
    const email = document.getElementById('newCoacheeEmail').value;
    const password = document.getElementById('newCoacheePassword').value;
    const message = document.getElementById('newEvaluationMessage').value;
    const dueDate = document.getElementById('newEvaluationDueDate').value;
    
    if (!name || !email || !password) {
        showNotification('Por favor completa todos los campos requeridos', 'error');
        return;
    }
    
    try {
        // Primero crear el coachee
        const createResponse = await fetch('/api/coach/create-coachee', {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({ name, email, password })
        });
        
        const createData = await createResponse.json();
        
        if (!createData.success) {
            showNotification(createData.error || 'Error al crear coachee', 'error');
            return;
        }
        
        // Luego asignar la evaluación
        const assignResponse = await fetch('/api/coach/assign-evaluation', {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({
                assessment_id: assessmentId,
                coachee_id: createData.coachee_id,
                message: message,
                due_date: dueDate
            })
        });
        
        const assignData = await assignResponse.json();
        
        if (assignData.success) {
            showNotification('Coachee creado y evaluación asignada exitosamente', 'success');
            const modal = bootstrap.Modal.getInstance(document.getElementById('assignEvaluationToCoacheeModal'));
            modal.hide();
        } else {
            showNotification(assignData.error || 'Error al asignar evaluación', 'error');
        }
    } catch (error) {
        console.error('Error:', error);
        showNotification('Error al procesar la solicitud', 'error');
    }
}

// Ver detalles de solicitud de plan de desarrollo
async function viewRequestDetails(requestId) {
    
    try {
        // Abrir modal y mostrar loading
        const modalElement = document.getElementById('devPlanRequestModal');
        if (!modalElement) {
            console.error('❌ Modal element not found!');
            showToast('Error: Modal no encontrado', 'error');
            return;
        }
        
        
        const modal = new bootstrap.Modal(modalElement);
        modal.show();
        
        
        const loadingEl = document.getElementById('devPlanModalLoading');
        const contentEl = document.getElementById('devPlanModalContent');
        
        if (loadingEl) {
            loadingEl.style.display = 'block';
        } else {
            console.error('❌ Loading element not found');
        }
        
        if (contentEl) {
            contentEl.style.display = 'none';
        } else {
            console.error('❌ Content element not found');
        }
        
        
        // Obtener detalles de la solicitud
        const response = await fetch(`/api/coach/development-plan-request/${requestId}`);
        
        const data = await response.json();
        
        
        if (!data.success) {
            console.error('❌ VIEW-REQUEST: API returned error:', data.error);
            showToast(data.error || 'Error al cargar detalles', 'error');
            modal.hide();
            return;
        }
        
        
        // Verificar que los elementos del modal existen
        const modalElements = {
            coacheeName: document.getElementById('devPlanCoacheeName'),
            coacheeEmail: document.getElementById('devPlanCoacheeEmail'),
            coacheeUsername: document.getElementById('devPlanCoacheeUsername'),
            requestDate: document.getElementById('devPlanRequestDate'),
            priority: document.getElementById('modalPriority'),
            priorityBadge: document.getElementById('modalPriorityBadge'),
            focusAreas: document.getElementById('modalFocusAreas'),
            evalInfo: document.getElementById('modalEvaluationInfo'),
            noEval: document.getElementById('modalNoEvaluation'),
            evalTitle: document.getElementById('modalEvalTitle'),
            evalScore: document.getElementById('modalEvalScore'),
            evalDate: document.getElementById('modalEvalDate'),
            goals: document.getElementById('modalGoals'),
            goalsCard: document.getElementById('modalGoalsCard'),
            customMessage: document.getElementById('modalCustomMessage')
        };
        
        // Log elementos que no se encuentran
        Object.keys(modalElements).forEach(key => {
            if (!modalElements[key]) {
                console.error(`❌ Elemento no encontrado: ${key}`);
            }
        });
        
        // Llenar información del coachee
        if (modalElements.coacheeName) {
            modalElements.coacheeName.textContent = data.coachee.name;
        } else {
            console.error('❌ modalCoacheeName element not found!');
        }
        if (modalElements.coacheeEmail) {
            modalElements.coacheeEmail.textContent = data.coachee.email;
        } else {
            console.error('❌ modalCoacheeEmail element not found!');
        }
        if (modalElements.coacheeUsername) {
            modalElements.coacheeUsername.textContent = data.coachee.username;
        } else {
            console.error('❌ modalCoacheeUsername element not found!');
        }
        
        // Guardar IDs para usar en la creación del plan
        const requestCoacheeIdEl = document.getElementById('requestCoacheeId');
        const currentRequestIdEl = document.getElementById('currentRequestId');
        
        if (requestCoacheeIdEl) {
            requestCoacheeIdEl.value = data.coachee.id;
        }
        
        if (currentRequestIdEl) {
            currentRequestIdEl.value = requestId;
        }
        
        const requestDate = new Date(data.task.created_at);
        if (modalElements.requestDate) {
            modalElements.requestDate.textContent = requestDate.toLocaleDateString('es-CL', {
                year: 'numeric',
                month: 'long',
                day: 'numeric',
                hour: '2-digit',
                minute: '2-digit'
            });
        }
        
        // Mostrar prioridad
        if (modalElements.priorityBadge && data.priority) {
            const priorityLabels = {
                'high': { text: 'Alta - Necesito apoyo urgente', class: 'bg-danger' },
                'medium': { text: 'Media - Desarrollo gradual', class: 'bg-warning text-dark' },
                'low': { text: 'Baja - Desarrollo a largo plazo', class: 'bg-info' }
            };
            
            const priority = priorityLabels[data.priority] || { text: data.priority, class: 'bg-secondary' };
            modalElements.priorityBadge.textContent = priority.text;
            modalElements.priorityBadge.className = `badge ${priority.class}`;
            modalElements.priorityBadge.style.fontSize = '1rem';
            modalElements.priorityBadge.style.padding = '0.5rem 1rem';
        }
        
        // Mostrar áreas de enfoque
        if (modalElements.focusAreas && data.focus_areas && data.focus_areas.length > 0) {
            modalElements.focusAreas.innerHTML = '';
            
            const focusIcons = {
                'Comunicación Efectiva': { icon: 'fa-comments', color: '#3b82f6' },
                'Liderazgo de Equipos': { icon: 'fa-users', color: '#10b981' },
                'Toma de Decisiones': { icon: 'fa-sitemap', color: '#f59e0b' },
                'Resolución de Conflictos': { icon: 'fa-handshake', color: '#06b6d4' },
                'Inteligencia Emocional': { icon: 'fa-heart', color: '#ef4444' },
                'Productividad Personal': { icon: 'fa-tasks', color: '#6b7280' }
            };
            
            data.focus_areas.forEach(area => {
                const areaInfo = focusIcons[area] || { icon: 'fa-check', color: '#8b5cf6' };
                const badge = document.createElement('span');
                badge.className = 'badge';
                badge.style.cssText = `background-color: ${areaInfo.color}; color: white; font-size: 0.875rem; padding: 0.5rem 0.75rem; font-weight: 500;`;
                badge.innerHTML = `<i class="fas ${areaInfo.icon} me-1"></i>${area}`;
                modalElements.focusAreas.appendChild(badge);
            });
        } else if (modalElements.focusAreas) {
            modalElements.focusAreas.innerHTML = '<span class="text-muted">No se especificaron áreas de enfoque</span>';
        }
        
        // Llenar información de la evaluación
        
        if (data.evaluation) {
            if (modalElements.evalInfo) modalElements.evalInfo.style.display = 'block';
            if (modalElements.noEval) modalElements.noEval.style.display = 'none';
            
            if (modalElements.evalTitle) modalElements.evalTitle.textContent = data.evaluation.assessment_title;
            if (modalElements.evalScore) modalElements.evalScore.textContent = data.evaluation.score + '%';
            
            if (data.evaluation.completed_at) {
                const completedDate = new Date(data.evaluation.completed_at);
                if (modalElements.evalDate) {
                    modalElements.evalDate.textContent = completedDate.toLocaleDateString('es-CL', {
                        year: 'numeric',
                        month: 'long',
                        day: 'numeric'
                    });
                }
            } else {
                if (modalElements.evalDate) modalElements.evalDate.textContent = 'Fecha no disponible';
            }
            
            // Color del badge según puntuación
            if (modalElements.evalScore) {
                const score = data.evaluation.score;
                if (score >= 80) {
                    modalElements.evalScore.className = 'badge bg-success';
                } else if (score >= 60) {
                    modalElements.evalScore.className = 'badge bg-primary';
                } else if (score >= 40) {
                    modalElements.evalScore.className = 'badge bg-warning';
                } else {
                    modalElements.evalScore.className = 'badge bg-danger';
                }
                modalElements.evalScore.style.fontSize = '1rem';
                modalElements.evalScore.style.padding = '0.5rem 1rem';
            }
        } else {
            if (modalElements.evalInfo) modalElements.evalInfo.style.display = 'none';
            if (modalElements.noEval) modalElements.noEval.style.display = 'block';
        }
        
        // Mostrar objetivos específicos
        if (modalElements.goals && modalElements.goalsCard) {
            if (data.goals && data.goals.trim()) {
                modalElements.goals.textContent = data.goals;
                modalElements.goalsCard.style.display = 'block';
            } else {
                modalElements.goalsCard.style.display = 'none';
            }
        }
        
        // Llenar mensaje personalizado (mensaje adicional del coachee)
        if (modalElements.customMessage) {
            // Extraer solo el mensaje adicional si existe en el formato estructurado
            let displayMessage = data.task.custom_message || 'Sin mensaje adicional';
            
            // Si el mensaje tiene el formato estructurado, extraer solo la parte del mensaje adicional
            if (displayMessage.includes('Mensaje adicional:')) {
                const parts = displayMessage.split('Mensaje adicional:');
                if (parts.length > 1) {
                    displayMessage = parts[1].trim();
                }
            }
            
            modalElements.customMessage.textContent = displayMessage;
        }
        
        // Ocultar loading y mostrar contenido
        document.getElementById('devPlanModalLoading').style.display = 'none';
        document.getElementById('devPlanModalContent').style.display = 'block';
        
    } catch (error) {
        console.error('❌ VIEW-REQUEST: Error loading details', error);
        showToast('Error al cargar detalles de la solicitud', 'error');
    }
}

