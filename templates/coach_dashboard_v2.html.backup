// Obtener información del video de YouTube
async function fetchYouTubeVideoInfo(videoId) {
    const titleInput = document.getElementById('contentTitle');
    const durationInput = document.getElementById('videoDuration');
    const descriptionInput = document.getElementById('contentDescription');
    const previewContainer = document.getElementById('videoPreview');
    
    try {
        
        let videoData = {
            title: '',
            author: '',
            duration: null,
            thumbnail: ''
        };
        
        let dataObtained = false;
        
        // Intentar con oEmbed API (más confiable para título y autor)
        try {
            const oembedResponse = await fetch(`https://www.youtube.com/oembed?url=https://www.youtube.com/watch?v=${videoId}&format=json`, {
                mode: 'cors',
                cache: 'no-cache'
            });
            
            if (oembedResponse.ok) {
                const oembedData = await oembedResponse.json();
                videoData.title = oembedData.title || '';
                videoData.author = oembedData.author_name || '';
                videoData.thumbnail = oembedData.thumbnail_url || '';
                dataObtained = true;
            }
        } catch (oembedError) {
            console.warn('⚠️ Error en oEmbed API:', oembedError.message);
        }
        
        // Si no obtuvimos datos, mostrar mensaje y salir
        if (!dataObtained || !videoData.title) {
            console.warn('⚠️ No se pudo obtener información del video');
            
            // Mostrar mensaje informativo en la vista previa
            if (previewContainer) {
                const currentContent = previewContainer.innerHTML;
                if (!currentContent.includes('Información no disponible')) {
                    previewContainer.innerHTML = currentContent + `
                        <div class="mt-3 p-3" style="background: #fff3cd; border-left: 4px solid #ffc107; border-radius: 8px; text-align: left;">
                            <div class="mb-2">
                                <i class="fas fa-exclamation-triangle me-2 text-warning"></i>
                                <strong>Información no disponible automáticamente</strong>
