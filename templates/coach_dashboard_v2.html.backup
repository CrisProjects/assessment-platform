            '<p class="small text-muted">' + (file.size / 1024).toFixed(2) + ' KB</p>';
    }
    
    // Función para actualizar contadores de estadísticas
    function updateDocumentStats(fileType, fileExtension) {
        
        const pdfCount = document.getElementById('pdfCount');
        const imageCount = document.getElementById('imageCount');
        const docCount = document.getElementById('docCount');
        
        if (!pdfCount || !imageCount || !docCount) {
