    /* === BOTTOM NAVIGATION BAR - INSTACOACH DESIGN === */
    .bottom-navigation {
        position: fixed !important;
        bottom: 0 !important;
        left: 0 !important;
        right: 0 !important;
        width: 100% !important;
        height: 72px !important;
        background: rgba(255, 255, 255, 0.95) !important;
        border-top: 1px solid rgba(229, 231, 235, 0.8) !important;
        display: flex !important;
        justify-content: space-around !important;
        align-items: center !important;
        z-index: 9999 !important;
        box-shadow: 0 -8px 24px rgba(102, 126, 234, 0.08), 0 -2px 8px rgba(0, 0, 0, 0.04) !important;
        backdrop-filter: blur(16px) !important;
        -webkit-backdrop-filter: blur(16px) !important;
        padding: 0 1rem !important;
    }
    
    .bottom-nav-item {
        flex: 1;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        gap: 0.25rem;
        border: none;
        background: transparent;
        cursor: pointer;
        padding: 0.625rem 0.5rem;
        transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
        color: #9ca3af;
        text-decoration: none;
        position: relative;
        border-radius: 12px;
        max-width: 100px;
    }
    
    .bottom-nav-item i {
        font-size: 1.375rem;
        transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    }
    
    .bottom-nav-item span {
        font-size: 0.6875rem;
        font-weight: 600;
        letter-spacing: 0.01em;
        transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    }
    
    .bottom-nav-item:hover {
        color: #667eea;
        background: rgba(102, 126, 234, 0.05);
        transform: translateY(-2px);
    }
    
    .bottom-nav-item:hover i {
        transform: scale(1.1);
    }
    
    .bottom-nav-item.active {
        color: #764ba2;
        background: linear-gradient(135deg, rgba(102, 126, 234, 0.1) 0%, rgba(118, 75, 162, 0.1) 100%);
    }
    
    .bottom-nav-item.active i {
        transform: scale(1.15);
        filter: drop-shadow(0 2px 4px rgba(118, 75, 162, 0.2));
    }
    
    .bottom-nav-item.active span {
        font-weight: 700;
    }
    
    .bottom-nav-item.active::before {
        content: '';
        position: absolute;
        top: 0;
        left: 50%;
        transform: translateX(-50%);
        width: 32px;
        height: 3px;
        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
        border-radius: 0 0 999px 999px;
        box-shadow: 0 2px 8px rgba(118, 75, 162, 0.4);
        animation: slideDown 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    }
    
    @keyframes slideDown {
        from {
            opacity: 0;
            transform: translateX(-50%) translateY(-4px);
        }
        to {
            opacity: 1;
            transform: translateX(-50%) translateY(0);
        }
    }
    
