// Función para inicializar listeners de video (llamada cuando el módulo se muestra)
// Variable global para evitar múltiples inicializaciones
let videoListenersInitialized = false;

function initializeVideoListeners() {
    
    const videoUrlInput = document.getElementById('videoUrl');
    if (videoUrlInput) {
        
        // Si ya se inicializaron, no hacer nada
        if (videoListenersInitialized) {
            return;
        }
        
        // Marcar como inicializado
        videoListenersInitialized = true;
        
        // Usar addEventListener con named functions para poder removerlos
        const inputHandler = function(e) {
            const url = e.target.value.trim();
            if (url) {
                updateVideoPreview(url);
            }
        };
        
        const pasteHandler = function(e) {
            setTimeout(() => {
                const url = e.target.value.trim();
                if (url) {
                    updateVideoPreview(url);
                }
            }, 100);
        };
        
        const blurHandler = function(e) {
            const url = e.target.value.trim();
            if (url) {
                updateVideoPreview(url);
            }
        };
        
        // Agregar listeners
        videoUrlInput.addEventListener('input', inputHandler);
        videoUrlInput.addEventListener('paste', pasteHandler);
        videoUrlInput.addEventListener('blur', blurHandler);
        
        // Guardar referencia para poder remover después si es necesario
        videoUrlInput._videoHandlers = { inputHandler, pasteHandler, blurHandler };
        
        
        // Si ya hay un valor en el input, procesarlo inmediatamente
        if (videoUrlInput.value.trim()) {
            updateVideoPreview(videoUrlInput.value.trim());
        }
        
    } else {
        console.error('❌ No se encontró el input de videoUrl en el DOM');
    }
}

// Inicializar listeners cuando el DOM esté listo
document.addEventListener('DOMContentLoaded', function() {
