<script>
// Llamar a la función cuando se cargue la pestaña de contenido asignado
document.addEventListener('DOMContentLoaded', () => {
    
    // ============================================
    // LISTENERS PARA FILTROS DE CONTENIDO ASIGNADO
    // ============================================
    
    // Esperar a que Alpine.js esté completamente cargado
    setTimeout(() => {
        // Aplicar filtros automáticamente cuando cambien los selectores
        const coacheeFilter = document.getElementById('coacheeFilter');
        const viewModeFilter = document.getElementById('viewModeFilter');
        const statusFilter = document.getElementById('statusFilter');
        const contentSearch = document.getElementById('contentSearch');
        
        if (coacheeFilter) {
            coacheeFilter.addEventListener('change', () => {
                applyContentFilters();
            });
        } else {
            console.warn('⚠️ No se encontró coacheeFilter');
        }
        
        if (viewModeFilter) {
            viewModeFilter.addEventListener('change', () => {
                applyContentFilters();
            });
        } else {
            console.warn('⚠️ No se encontró viewModeFilter');
        }
        
        if (statusFilter) {
            statusFilter.addEventListener('change', () => {
                applyContentFilters();
            });
        } else {
            console.warn('⚠️ No se encontró statusFilter');
        }
        
        if (contentSearch) {
            // Aplicar filtro al presionar Enter
            contentSearch.addEventListener('keypress', (e) => {
                if (e.key === 'Enter') {
                    e.preventDefault();
                    applyContentFilters();
                }
            });
            
            // También aplicar filtro con un pequeño delay al escribir
            let searchTimeout;
            contentSearch.addEventListener('input', () => {
                clearTimeout(searchTimeout);
                searchTimeout = setTimeout(() => {
                    if (contentSearch.value.length >= 2 || contentSearch.value.length === 0) {
                        applyContentFilters();
                    }
                }, 500); // 500ms de delay después de dejar de escribir
            });
        } else {
            console.warn('⚠️ No se encontró contentSearch');
        }
        
    }, 1000); // Esperar 1 segundo para asegurar que Alpine.js está listo
});

</script>

