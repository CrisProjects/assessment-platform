            return;
        }
        
        // Resetear contadores
        pdfCount.textContent = '0';
        imageCount.textContent = '0';
        docCount.textContent = '0';
        
        // Incrementar el contador apropiado
        if (fileType === 'application/pdf' || fileExtension === 'pdf') {
            pdfCount.textContent = '1';
        } else if (fileType.startsWith('image/') || ['jpg', 'jpeg', 'png', 'gif'].includes(fileExtension)) {
            imageCount.textContent = '1';
        } else {
            docCount.textContent = '1';
        }
        
    }
    
    // Event listener para cambio de archivo
    const documentFileInput = document.getElementById('documentFile');
    if (documentFileInput) {
        documentFileInput.addEventListener('change', function(e) {
