                    document.getElementById('noteDate').valueAsDate = new Date();
                    
                    // Actualizar lista de notas
                    this.renderNotesList(saveData.notes);
                } else {
                    this.showNotification(saveData.error || 'Error al guardar nota', 'error');
                }
            } catch (error) {
                console.error('Error saving note:', error);
                this.showNotification('Error de conexión al guardar nota', 'error');
            }
