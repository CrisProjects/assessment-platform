        renderDevelopmentPlanRequests() {
            const tableBody = document.getElementById('devPlanRequestsTableBody');
            const requestsCount = document.getElementById('requestsCount');
            
            if (!tableBody) return;
            
            if (!this.developmentPlanRequests || this.developmentPlanRequests.length === 0) {
                tableBody.innerHTML = '<tr><td colspan="5" class="text-center text-muted" style="padding: 3rem;">No hay solicitudes pendientes</td></tr>';
                if (requestsCount) requestsCount.style.display = 'none';
                return;
            }
            
            // Update badge count
            if (requestsCount) {
                requestsCount.textContent = this.developmentPlanRequests.length;
                requestsCount.style.display = 'inline-block';
            }
            
            tableBody.innerHTML = this.developmentPlanRequests.map(req => {
