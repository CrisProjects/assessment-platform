// Publish draft (placeholder)
async function publishDraft(planId) {
    if (!confirm('¿Estás seguro de que quieres publicar este plan? El coachee podrá verlo inmediatamente.')) {
        return;
    }
    
    try {
        
        const response = await fetch(`/api/coach/development-plan/${planId}/publish`, {
            method: 'PUT',
            headers: {
                'Content-Type': 'application/json'
            }
        });
        
        const result = await response.json();
        
        if (response.ok) {
            showToast('Plan publicado exitosamente. El coachee ya puede verlo.', 'success');
            
            // Reload both lists to update UI
            loadDraftPlans();
            loadDevelopmentPlans();
            
        } else {
            throw new Error(result.error || 'Error al publicar el plan');
        }
        
    } catch (error) {
        console.error('❌ PUBLISH-DRAFT: Error:', error);
        showToast('Error al publicar el plan: ' + error.message, 'error');
    }
}

