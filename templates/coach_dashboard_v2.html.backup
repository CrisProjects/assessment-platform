    // Función para actualizar vista previa de documentos
    function updateDocumentPreview(file) {
        
        const previewContainer = document.getElementById('documentPreviewContainer');
        if (!previewContainer) {
            console.error('❌ No se encontró el elemento documentPreviewContainer');
            return;
        }
        
        
        const fileType = file.type;
        const fileName = file.name;
        const fileExtension = fileName.split('.').pop().toLowerCase();
        
        
        // Actualizar contadores
        updateDocumentStats(fileType, fileExtension);
        
        // Vista previa según tipo de archivo
        if (fileType === 'application/pdf' || fileExtension === 'pdf') {
            renderPDFPreview(file, previewContainer);
        } else if (fileType.startsWith('image/') || ['jpg', 'jpeg', 'png', 'gif'].includes(fileExtension)) {
            renderImagePreview(file, previewContainer);
        } else {
            // Para otros tipos de archivos (doc, docx, etc.)
            renderDocumentIcon(file, previewContainer);
        }
    }
    
    // Función para renderizar vista previa de PDF
    async function renderPDFPreview(file, container) {
        
        container.innerHTML = '<div class="text-center">' +
            '<div class="spinner-border text-primary mb-2" role="status">' +
            '<span class="visually-hidden">Cargando...</span>' +
            '</div>' +
            '<p class="small">Cargando vista previa del PDF...</p>' +
            '</div>';
        
        try {
            const arrayBuffer = await file.arrayBuffer();
            const pdf = await pdfjsLib.getDocument({ data: arrayBuffer }).promise;
            const page = await pdf.getPage(1);
            
            // Dimensiones máximas del canvas (como en coach_feed y coachee_dashboard)
            const maxWidth = 800;
            const maxHeight = 600;
            
            // Obtener viewport original
            const viewport = page.getViewport({ scale: 1 });
            
            // Calcular escala para ajustar al contenedor
            const scaleX = maxWidth / viewport.width;
            const scaleY = maxHeight / viewport.height;
            const scale = Math.min(scaleX, scaleY);
            
            // Crear viewport escalado
            const scaledViewport = page.getViewport({ scale });
            
            // Crear canvas con dimensiones fijas
            const canvas = document.createElement('canvas');
            const context = canvas.getContext('2d');
            canvas.width = maxWidth;
            canvas.height = maxHeight;
            
            // Fondo blanco
            context.fillStyle = '#ffffff';
            context.fillRect(0, 0, maxWidth, maxHeight);
            
            // Centrar el PDF en el canvas
            const offsetX = (maxWidth - scaledViewport.width) / 2;
            const offsetY = (maxHeight - scaledViewport.height) / 2;
            
            // Renderizar con offset para centrar
            await page.render({
                canvasContext: context,
                viewport: scaledViewport,
                transform: [1, 0, 0, 1, offsetX, offsetY]
            }).promise;
            
            // Aplicar estilo responsive al canvas
            canvas.style.width = '100%';
            canvas.style.height = 'auto';
            canvas.style.maxWidth = maxWidth + 'px';
            canvas.style.display = 'block';
            canvas.style.margin = '0 auto';
            
            container.innerHTML = '<div style="max-height: 600px; overflow-y: auto; text-align: center;">' +
                '</div>' +
                '<p class="small text-muted mt-2 text-center">' +
                '<i class="fas fa-file-pdf text-danger me-1"></i>' +
                file.name + ' (' + (file.size / 1024).toFixed(2) + ' KB)' +
                '</p>';
            
            const canvasDiv = container.querySelector('div');
            canvasDiv.appendChild(canvas);
            
        } catch (error) {
            console.error('❌ Error renderizando PDF:', error);
            container.innerHTML = '<i class="fas fa-file-pdf text-danger fa-3x mb-2"></i>' +
                '<p class="small">Error al cargar vista previa del PDF</p>' +
                '<p class="small text-muted">' + file.name + '</p>';
        }
    }
    
    // Función para renderizar vista previa de imagen
    function renderImagePreview(file, container) {
        
        container.innerHTML = '<div class="text-center">' +
            '<div class="spinner-border text-primary mb-2" role="status">' +
            '<span class="visually-hidden">Cargando...</span>' +
            '</div>' +
            '<p class="small">Cargando imagen...</p>' +
            '</div>';
        
        const reader = new FileReader();
        
        reader.onload = function(e) {
            container.innerHTML = '<div style="max-height: 600px; overflow-y: auto; text-align: center; padding: 1rem; background: #f8f9fa; border-radius: 8px;">' +
                '<img src="' + e.target.result + '" ' +
                'alt="Vista previa" ' +
                'style="max-width: 100%; max-height: 550px; height: auto; border-radius: 8px; box-shadow: 0 2px 8px rgba(0,0,0,0.1);">' +
                '</div>' +
                '<p class="small text-muted mt-2 text-center">' +
                '<i class="fas fa-image text-info me-1"></i>' +
                file.name + ' (' + (file.size / 1024).toFixed(2) + ' KB)' +
                '</p>';
        };
        
        reader.onerror = function() {
            console.error('❌ Error leyendo imagen');
            container.innerHTML = '<i class="fas fa-image text-info fa-3x mb-2"></i>' +
                '<p class="small">Error al cargar vista previa de la imagen</p>' +
                '<p class="small text-muted">' + file.name + '</p>';
        };
        
        reader.readAsDataURL(file);
    }
    
    // Función para mostrar icono de documento
    function renderDocumentIcon(file, container) {
        
        const fileExtension = file.name.split('.').pop().toLowerCase();
        let iconClass = 'fa-file';
        let iconColor = 'text-secondary';
        
        if (['doc', 'docx'].includes(fileExtension)) {
            iconClass = 'fa-file-word';
