 */
async function publishDraftAssessment(assessmentId) {
    
    if (!confirm('¿Estás seguro de que deseas publicar esta evaluación? Una vez publicada, estará disponible para asignar a coachees.')) {
        return;
    }
    
    try {
        const response = await fetch(`/api/coach/assessments/${assessmentId}/publish`, {
            method: 'PUT',
            headers: {
                'Content-Type': 'application/json'
            }
        });
        
        const result = await response.json();
        
        if (response.ok) {
            showToast('Evaluación publicada exitosamente. Ya está disponible para asignar.', 'success');
            
            // Reload drafts
            loadDraftAssessments();
            
        } else {
            throw new Error(result.error || 'Error al publicar la evaluación');
        }
        
    } catch (error) {
        console.error('❌ PUBLISH-DRAFT: Error:', error);
        showToast('Error al publicar: ' + error.message, 'error');
    }
}

