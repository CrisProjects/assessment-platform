<script>
// Global toast function for wizard
function showToast(message, type = 'info') {
    // Map type names
    const typeMap = {
        'warning': 'warning',
        'error': 'danger',
        'success': 'success',
        'info': 'info'
    };
    
    const bootstrapType = typeMap[type] || 'info';
    
    // Get or create toast container
    let toastContainer = document.getElementById('toastContainer');
    if (!toastContainer) {
        toastContainer = document.createElement('div');
        toastContainer.id = 'toastContainer';
        toastContainer.className = 'toast-container position-fixed top-0 end-0 p-3';
        toastContainer.style.zIndex = '9999';
        document.body.appendChild(toastContainer);
    }
    
    // Create toast
    const toast = document.createElement('div');
    toast.className = `toast align-items-center text-white bg-${bootstrapType} border-0`;
    toast.setAttribute('role', 'alert');
    toast.innerHTML = `
        <div class="d-flex">
            <div class="toast-body">${message}</div>
            <button type="button" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast"></button>
        </div>
    `;
    
    toastContainer.appendChild(toast);
    const bsToast = new bootstrap.Toast(toast);
    bsToast.show();
    
    // Remove after 5 seconds
    setTimeout(() => toast.remove(), 5000);
}

// Wizard state management
let currentStep = 1;
let selectedAreas = [];
let acciones = [{descripcion: '', frecuencia: '', fecha_objetivo: ''}];
let currentCoacheeId = null;
let currentRequestTaskId = null;

// Open wizard modal
function openCreatePlanModal() {
    // Get coachee ID from request details modal
    const coacheeIdEl = document.getElementById('requestCoacheeId');
    const requestIdEl = document.getElementById('currentRequestId');
    
    if (coacheeIdEl && coacheeIdEl.value) {
        currentCoacheeId = parseInt(coacheeIdEl.value);
    }
    
    if (requestIdEl && requestIdEl.value) {
        currentRequestTaskId = parseInt(requestIdEl.value);
    }
    
    // Close request details modal
    const requestModal = bootstrap.Modal.getInstance(document.getElementById('devPlanRequestModal'));
    if (requestModal) {
        requestModal.hide();
    }
    
    // Reset wizard
    resetWizard();
    
    // Setup coachee selection
    setupCoacheeSelection();
    
    // Open wizard modal
    const modal = new bootstrap.Modal(document.getElementById('createDevPlanModal'));
    modal.show();
}

// Setup coachee selection
async function setupCoacheeSelection() {
    
    const coacheeSelector = document.getElementById('coacheeSelector');
    const selectedDisplay = document.getElementById('selectedCoacheeDisplay');
    
    if (!coacheeSelector || !selectedDisplay) {
        console.error('❌ SETUP-COACHEE: Elements not found!', { coacheeSelector, selectedDisplay });
        return;
    }
    
    if (currentCoacheeId) {
        // Pre-selected coachee from request
        coacheeSelector.style.display = 'none';
        selectedDisplay.style.display = 'block';
        
        // Fetch coachee name
        try {
            const response = await fetch('/api/coach/coachees');
            const data = await response.json();
            
            if (data.coachees && Array.isArray(data.coachees)) {
                const selectedCoachee = data.coachees.find(c => c.id === currentCoacheeId);
                if (selectedCoachee) {
                    document.getElementById('selectedCoacheeName').textContent = selectedCoachee.name;
                } else {
                    console.warn('⚠️ SETUP-COACHEE: Coachee not found in list, ID:', currentCoacheeId);
                }
            } else {
                console.error('❌ SETUP-COACHEE: Invalid response format:', data);
            }
        } catch (error) {
            console.error('❌ SETUP-COACHEE: Error fetching coachee:', error);
        }
    } else {
        // No pre-selected coachee, show selector
        coacheeSelector.style.display = 'block';
        selectedDisplay.style.display = 'none';
        
        // Load coachees list
        try {
            const response = await fetch('/api/coach/coachees');
            
            if (!response.ok) {
                throw new Error(`HTTP ${response.status}`);
            }
            
            const data = await response.json();
            
            if (data.coachees && Array.isArray(data.coachees)) {
                const select = document.getElementById('selectCoachee');
                select.innerHTML = '<option value="">Selecciona un coachee...</option>';
                
                if (data.coachees.length === 0) {
                    const option = document.createElement('option');
                    option.value = '';
                    option.textContent = 'No hay coachees disponibles';
                    option.disabled = true;
                    select.appendChild(option);
                    console.warn('⚠️ SETUP-COACHEE: No coachees found');
                } else {
                    data.coachees.forEach(coachee => {
                        const option = document.createElement('option');
                        option.value = coachee.id;
                        option.textContent = `${coachee.name} (${coachee.email})`;
                        select.appendChild(option);
                    });
                }
                
            } else {
                console.error('❌ SETUP-COACHEE: Invalid response format:', data);
                showToast('Error: formato de respuesta inválido', 'error');
            }
        } catch (error) {
            console.error('❌ SETUP-COACHEE: Error loading coachees:', error);
            showToast('Error al cargar lista de coachees: ' + error.message, 'error');
        }
    }
}

// Handle coachee selection
function handleCoacheeSelection() {
    const select = document.getElementById('selectCoachee');
    const selectedId = parseInt(select.value);
    
    
    if (selectedId) {
        currentCoacheeId = selectedId;
        
        // Update display
        const selectedDisplay = document.getElementById('selectedCoacheeDisplay');
        const coacheeSelector = document.getElementById('coacheeSelector');
        const selectedOption = select.options[select.selectedIndex];
        
        
        document.getElementById('selectedCoacheeName').textContent = selectedOption.text;
        coacheeSelector.style.display = 'none';
        selectedDisplay.style.display = 'block';
        
    } else {
    }
}

// Close wizard modal
function closeCreatePlanModal() {
    if (confirm('¿Seguro que quieres cerrar? Se perderán los cambios no guardados.')) {
        const modal = bootstrap.Modal.getInstance(document.getElementById('createDevPlanModal'));
        modal.hide();
        resetWizard();
    }
}

// Reset wizard state
function resetWizard() {
    currentStep = 1;
    selectedAreas = [];
    acciones = [{descripcion: '', frecuencia: '', fecha_objetivo: ''}];
    
    document.getElementById('objetivo').value = '';
    document.getElementById('situacion').value = '';
    document.getElementById('indicadores').value = '';
    
    // Reset coachee selector and display
    const coacheeSelector = document.getElementById('coacheeSelector');
    const selectedDisplay = document.getElementById('selectedCoacheeDisplay');
    if (coacheeSelector) coacheeSelector.style.display = 'none';
    if (selectedDisplay) selectedDisplay.style.display = 'none';
    
    // Reset area cards
    document.querySelectorAll('.area-card').forEach(card => {
        card.classList.remove('selected');
        card.style.borderColor = '#e2e8f0';
        card.style.background = 'white';
        const check = card.querySelector('.area-check');
        check.style.background = 'transparent';
        check.style.borderColor = '#cbd5e0';
        check.querySelector('i').style.display = 'none';
    });
    
    renderAcciones();
    goToStep(1);
}

// Navigation
function nextStep() {
    
    if (validateStep(currentStep)) {
        if (currentStep < 5) {
            goToStep(currentStep + 1);
        }
    } else {
    }
}

function previousStep() {
    if (currentStep > 1) {
        goToStep(currentStep - 1);
    }
}

function goToStep(step) {
    // Hide all steps
    document.querySelectorAll('.wizard-content').forEach(el => el.style.display = 'none');
    
    // Show target step
    document.getElementById(`step${step}`).style.display = 'block';
    
    // Update progress indicator
    document.querySelectorAll('.wizard-step').forEach((el, index) => {
        const stepNum = index + 1;
        const stepCircle = el.querySelector('.step-number');
        const stepLabel = el.querySelector('.step-label');
        
        if (stepNum < step) {
            // Completed step
            stepCircle.style.background = '#48bb78';
            stepCircle.style.color = 'white';
            stepLabel.style.color = '#48bb78';
            stepCircle.classList.add('active');
        } else if (stepNum === step) {
            // Current step
            stepCircle.style.background = '#667eea';
            stepCircle.style.color = 'white';
            stepLabel.style.color = '#667eea';
            stepCircle.classList.add('active');
        } else {
            // Future step
            stepCircle.style.background = '#e9ecef';
            stepCircle.style.color = '#6c757d';
            stepLabel.style.color = '#6c757d';
            stepCircle.classList.remove('active');
        }
    });
    
    // Update connector lines
    document.querySelectorAll('.step-connector').forEach((connector, index) => {
        if (index < step - 1) {
            connector.style.background = '#48bb78';
        } else {
            connector.style.background = '#e9ecef';
        }
    });
    
    // Update buttons
    document.getElementById('btnPrevious').style.display = step > 1 ? 'inline-block' : 'none';
    document.getElementById('btnNext').style.display = step < 5 ? 'inline-block' : 'none';
    document.getElementById('btnPublish').style.display = step === 5 ? 'inline-block' : 'none';
    
    // Render summary on step 5
    if (step === 5) {
        renderPlanSummary();
    }
    
    currentStep = step;
}

// Validation
function validateStep(step) {
    
    switch(step) {
        case 1:
            
            // Check coachee selection
            if (!currentCoacheeId) {
                showToast('Por favor selecciona un coachee', 'warning');
                return false;
            }
            
            const objetivo = document.getElementById('objetivo').value.trim();
            
            if (!objetivo) {
                showToast('Por favor completa el objetivo de desarrollo', 'warning');
                return false;
            }
            if (objetivo.length < 20) {
                showToast('El objetivo debe tener al menos 20 caracteres para ser significativo', 'warning');
                return false;
            }
            return true;
            
        case 2:
            // Optional, always valid
            return true;
            
        case 3:
            if (selectedAreas.length === 0) {
                showToast('Selecciona al menos 1 área de desarrollo', 'warning');
                return false;
            }
            return true;
            
        case 4:
            const validAcciones = acciones.filter(a => 
                a.descripcion && a.descripcion.trim().length > 0
            );
            if (validAcciones.length === 0) {
                showToast('Debes definir al menos 1 acción concreta', 'warning');
                return false;
            }
            // Update acciones to only valid ones
            acciones = validAcciones;
            return true;
            
        case 5:
            // Optional, always valid
            return true;
            
        default:
            return true;
    }
}

// Areas management
function toggleArea(areaName) {
    const index = selectedAreas.indexOf(areaName);
    const card = document.querySelector(`.area-card[data-area="${areaName}"]`);
    const check = card.querySelector('.area-check');
    const checkIcon = check.querySelector('i');
    
    if (index > -1) {
        // Remove area
        selectedAreas.splice(index, 1);
        card.classList.remove('selected');
        card.style.borderColor = '#e2e8f0';
        card.style.background = 'white';
        check.style.background = 'transparent';
        check.style.borderColor = '#cbd5e0';
        checkIcon.style.display = 'none';
    } else {
        // Add area
        if (selectedAreas.length >= 3) {
            showToast('Puedes seleccionar máximo 3 áreas', 'warning');
            return;
        }
        selectedAreas.push(areaName);
        card.classList.add('selected');
        card.style.borderColor = '#667eea';
        card.style.background = '#f7fafc';
        check.style.background = '#667eea';
        check.style.borderColor = '#667eea';
        checkIcon.style.display = 'block';
    }
}

// Acciones management
function addAccion() {
    acciones.push({descripcion: '', frecuencia: '', fecha_objetivo: ''});
    renderAcciones();
}

function removeAccion(index) {
    if (acciones.length > 1) {
        acciones.splice(index, 1);
        renderAcciones();
    } else {
        showToast('Debe haber al menos 1 acción', 'warning');
    }
}

function updateAccion(index, field, value) {
    acciones[index][field] = value;
}

function renderAcciones() {
    const container = document.getElementById('accionesContainer');
    container.innerHTML = acciones.map((accion, index) => `
