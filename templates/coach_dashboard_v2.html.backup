console.log('🔧 [SCRIPT] Iniciando carga del script dashboard...');

// ============================================
// ALPINE.JS DASHBOARD COMPONENT - V2
// ============================================

// Interceptar todas las peticiones fetch para verificar sesiones expiradas
(function() {
    const originalFetch = window.fetch;
    window.fetch = function(...args) {
        return originalFetch.apply(this, args)
            .then(async response => {
                // Verificar si hay sesión expirada en la respuesta
                if (response.ok && response.headers.get('content-type')?.includes('application/json')) {
                    // Clonar la respuesta para poder leerla sin consumirla
                    const clonedResponse = response.clone();
                    try {
                        const data = await clonedResponse.json();
                        // Verificar si la sesión expiró
                        if (data && data.session_expired && data.redirect_url) {
                            window.location.href = data.redirect_url;
                            return new Promise(() => {}); // Never resolves, ya que se redirige
                        }
                    } catch (e) {
                        // Si falla el parsing, continuar con la respuesta original
                    }
                }
                return response;
            });
    };
})();

// ============================================
// DROPDOWN UTILITIES
// ============================================
function toggleDashboardDropdown(event) {
    event.stopPropagation();
    const menu = document.getElementById('dashboardProfileMenu');
    if (menu) {
        menu.style.display = menu.style.display === 'none' ? 'block' : 'none';
    }
}

// ============================================
// MOBILE SIDEBAR UTILITIES
// ============================================
function toggleMobileSidebar() {
    const sidebar = document.getElementById('sidebarContainer');
    const backdrop = document.getElementById('sidebarBackdrop');
    const body = document.body;
    const button = document.getElementById('mobileMenuToggle');
    
    if (sidebar && backdrop) {
        const isOpen = sidebar.classList.contains('sidebar-open');
        
        if (isOpen) {
            // Cerrar
            sidebar.classList.remove('sidebar-open');
            backdrop.classList.remove('show');
            body.classList.remove('sidebar-open');
            if (button) {
                button.innerHTML = '<i class="fas fa-bars me-2"></i>Menú';
            }
        } else {
            // Abrir
            sidebar.classList.add('sidebar-open');
            backdrop.classList.add('show');
            body.classList.add('sidebar-open');
            if (button) {
                button.innerHTML = '<i class="fas fa-times me-2"></i>Cerrar';
            }
            
            // Agregar listener al backdrop para cerrar
            setTimeout(() => {
                backdrop.onclick = function(e) {
                    if (e.target === backdrop) {
                        toggleMobileSidebar();
                    }
                };
            }, 100);
        }
    }
}

// Cerrar sidebar al cambiar de módulo (opcional, mejora UX)
window.closeMobileSidebarOnNavigation = function() {
    if (window.innerWidth < 768) {
        const sidebar = document.getElementById('sidebarContainer');
        if (sidebar && sidebar.classList.contains('sidebar-open')) {
            toggleMobileSidebar();
        }
    }
};

// Cerrar dropdown al hacer clic fuera
document.addEventListener('click', function(event) {
    const dropdown = document.getElementById('dashboardProfileDropdown');
    const menu = document.getElementById('dashboardProfileMenu');
    if (dropdown && menu && !dropdown.contains(event.target)) {
        menu.style.display = 'none';
    }
});

// ============================================
// GLOBAL FUNCTION WRAPPERS FOR ALPINE.JS
// ============================================
// Estas funciones acceden al objeto Alpine.js una vez que está montado
window.openChangePasswordModal = function() {
    const alpineData = Alpine.$data(document.querySelector('[x-data]'));
    if (alpineData && alpineData.openChangePasswordModal) {
        alpineData.openChangePasswordModal();
    } else {
        console.error('❌ No se pudo acceder a openChangePasswordModal');
    }
};

window.logout = function() {
    const alpineData = Alpine.$data(document.querySelector('[x-data]'));
    if (alpineData && alpineData.logout) {
        alpineData.logout();
    } else {
