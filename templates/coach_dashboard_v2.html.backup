        // Renderizar contenido asignado (igual que Mi Contenido del coachee)
        renderAssignedContent(contentList) {
            const container = document.getElementById('contentListContainer');
            const emptyState = document.getElementById('contentEmptyState');
            
            if (!container) return;
            
            if (!contentList || contentList.length === 0) {
                container.innerHTML = '';
                if (emptyState) emptyState.style.display = 'block';
                
                // Actualizar contador
                const resultsCount = document.getElementById('contentResultsCount');
                if (resultsCount) {
                    resultsCount.textContent = '0 resultados';
                    resultsCount.className = 'badge bg-secondary';
                }
                return;
            }
            
            if (emptyState) emptyState.style.display = 'none';
            
            // Actualizar contador
            const resultsCount = document.getElementById('contentResultsCount');
            if (resultsCount) {
                const label = contentList.length === 1 ? 'resultado' : 'resultados';
                resultsCount.textContent = `${contentList.length} ${label}`;
                resultsCount.className = 'badge bg-primary';
            }
            
            let html = '';
            
            contentList.forEach(content => {
                const isVideo = content.content_type === 'video';
                const isDocument = content.content_type === 'document';
                
                // Detectar YouTube
                let youtubeVideoId = null;
                if (content.content_url) {
                    if (content.content_url.includes('youtube.com/watch?v=')) {
                        youtubeVideoId = content.content_url.split('v=')[1]?.split('&')[0];
                    } else if (content.content_url.includes('youtu.be/')) {
                        youtubeVideoId = content.content_url.split('youtu.be/')[1]?.split('?')[0];
                    }
                }
                
                // Detectar Instagram
                let instagramVideoId = null;
                if (content.content_url) {
                    const instagramPatterns = [
                        /instagram\.com\/(?:p|reel|tv)\/([A-Za-z0-9_-]+)/,
                        /instagram\.com\/(?:p|reel|tv)\/([A-Za-z0-9_-]+)\//
                    ];
                    for (const pattern of instagramPatterns) {
                        const match = content.content_url.match(pattern);
                        if (match && match[1]) {
                            instagramVideoId = match[1];
                            break;
                        }
                    }
                }
                
                // Construir HTML de vista previa
                let videoEmbedHTML = '';
                
                if (youtubeVideoId) {
                    videoEmbedHTML = `
                    <div class="content-preview has-embedded-video">
                        <div class="ratio ratio-16x9" style="position: relative; border-radius: 8px; overflow: hidden;">
                            <iframe 
                                src="https://www.youtube.com/embed/${youtubeVideoId}?rel=0&modestbranding=1" 
                                title="${content.title || 'Video'}"
                                frameborder="0" 
                                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
                                allowfullscreen
                                style="border-radius: 8px; width: 100%; height: 100%;">
                            </iframe>
                        </div>
                    </div>
                    `;
                } else if (instagramVideoId) {
                    videoEmbedHTML = `
                    <div class="content-preview has-embedded-video">
                        <div class="ratio ratio-9x16" style="position: relative; border-radius: 8px; overflow: hidden;">
                            <iframe 
                                src="https://www.instagram.com/p/${instagramVideoId}/embed" 
                                title="${content.title || 'Video'}"
                                frameborder="0" 
                                scrolling="no" 
                                allowtransparency="true"
                                style="border-radius: 8px; width: 100%; height: 100%;">
                            </iframe>
                        </div>
                    </div>
                    `;
                } else if (isDocument && content.content_url) {
                    videoEmbedHTML = `
                        <div class="content-preview">
                            <div style="width: 100%; height: 300px; background: #f5f5f5; overflow-y: auto; overflow-x: hidden; -webkit-overflow-scrolling: touch; position: relative; border-radius: 8px;">
                                <canvas id="doc-preview-assigned-${content.id}"
                                        width="1200"
                                        height="1000"
                                        style="width: 100%; height: auto; display: block; cursor: pointer;"
                                        onclick="window.open('${content.content_url}', '_blank')">
                                </canvas>
                                <div id="doc-loading-assigned-${content.id}" style="position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); text-align: center; color: #6c757d; pointer-events: none;">
                                    <div class="spinner-border" role="status" style="width: 3rem; height: 3rem;">
                                        <span class="visually-hidden">Cargando...</span>
                                    </div>
                                    <p class="mt-2">Cargando documento...</p>
                                </div>
                            </div>
                        </div>
                    `;
                }
                
