        // Crear invitación (coachee nuevo)
        async createInvitation(formData) {
            try {
                const response = await fetch('/api/coach/create-invitation-v2', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify(formData)
                });
                
                const data = await response.json();
                
                if (data.success) {
                    this.currentInvitationData = data.coachee;
