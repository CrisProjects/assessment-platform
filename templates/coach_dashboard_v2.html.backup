// Extraer ID de video de YouTube o Instagram desde URL
function extractYouTubeVideoId(url) {
    if (!url) return null;
    
    // Patrones para diferentes formatos de URL de YouTube
    const patterns = [
        /(?:youtube\.com\/watch\?v=|youtu\.be\/|youtube\.com\/embed\/)([^&\n?#]+)/,
        /youtube\.com\/watch\?.*v=([^&\n?#]+)/,
    ];
    
    for (const pattern of patterns) {
        const match = url.match(pattern);
        if (match && match[1]) {
            return match[1];
        }
    }
    
    return null;
}

// Extraer ID de video/reel de Instagram desde URL
function extractInstagramVideoId(url) {
    if (!url) return null;
    
    // Patrones para diferentes formatos de URL de Instagram
    const patterns = [
        /instagram\.com\/(?:p|reel|tv)\/([A-Za-z0-9_-]+)/,
        /instagram\.com\/(?:p|reel|tv)\/([A-Za-z0-9_-]+)\//,
    ];
    
    for (const pattern of patterns) {
        const match = url.match(pattern);
        if (match && match[1]) {
            return match[1];
        }
    }
    
    return null;
}

// Detectar tipo de plataforma de video
function detectVideoPlatform(url) {
    if (!url) return null;
    
    if (url.includes('youtube.com') || url.includes('youtu.be')) {
        return 'youtube';
    }
    if (url.includes('instagram.com')) {
        return 'instagram';
    }
    
    return null;
}

