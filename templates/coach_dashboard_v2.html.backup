// Save draft
async function saveDraft() {
    // Validate required fields
    if (!validateStep(1) || !validateStep(3) || !validateStep(4)) {
        showToast('Completa al menos el objetivo, áreas y acciones para guardar un borrador', 'warning');
        return;
    }
    
    await submitPlan('draft');
}

// Publish plan
async function publishPlan() {
    // Validate all steps
    for (let i = 1; i <= 5; i++) {
        if (!validateStep(i)) {
            return;
        }
    }
    
    await submitPlan('published');
}

// Submit plan to API
async function submitPlan(status) {
    try {
        const data = {
            coachee_id: currentCoacheeId,
            request_task_id: currentRequestTaskId,
            objetivo: document.getElementById('objetivo').value.trim(),
            situacion_actual: document.getElementById('situacion').value.trim() || null,
            areas_desarrollo: selectedAreas,
            acciones: acciones.map(a => ({
                descripcion: a.descripcion.trim(),
                frecuencia: a.frecuencia || null,
                fecha_objetivo: a.fecha_objetivo || null
            })),
            indicadores: document.getElementById('indicadores').value.trim() || null,
            status: status
        };
        
        
        const response = await fetch('/api/coach/development-plan', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify(data)
        });
        
        const result = await response.json();
        
        if (response.ok) {
            const message = status === 'draft' 
                ? 'Borrador guardado exitosamente'
                : 'Plan publicado exitosamente';
            showToast(message, 'success');
            
            // Close modal
            const modal = bootstrap.Modal.getInstance(document.getElementById('createDevPlanModal'));
            modal.hide();
            resetWizard();
            
            // Reload plans lists
            loadDevelopmentPlans();
            loadDraftPlans();
            
        } else {
            throw new Error(result.error || 'Error al guardar el plan');
        }
        
    } catch (error) {
        console.error('Error submitting plan:', error);
        showToast('Error al guardar el plan: ' + error.message, 'error');
    }
}

