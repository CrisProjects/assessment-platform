            send_notification: document.getElementById('directSendNotification').checked
        };
        
        const response = await fetch('/api/coach/direct-appointment', {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify(data)
        });
        
        const result = await response.json();
        
        if (result.success) {
            showNotification(result.message || 'Cita enviada exitosamente', 'success');
            form.reset();
            // Refrescar calendario
            if (window.coachDashboard && window.coachDashboard.calendar) {
                window.coachDashboard.calendar.refetchEvents();
            }
        } else {
            showNotification(result.error || 'Error al enviar cita', 'error');
        }
    } catch (error) {
        console.error('Error:', error);
        showNotification('Error al enviar cita', 'error');
    } finally {
        submitBtn.disabled = false;
        submitBtn.innerHTML = originalText;
    }
}

async function loadCoacheesForDirectAppointment() {
    const select = document.getElementById('directCoacheeId');
    if (!select) return;
    
    try {
        const response = await fetch('/api/coach/my-coachees');
        const data = await response.json();
        
        if (data.success && data.coachees) {
            select.innerHTML = '<option value="">Seleccionar coachee...</option>';
            data.coachees.forEach(coachee => {
                const option = document.createElement('option');
                option.value = coachee.id;
                option.textContent = coachee.full_name;
                select.appendChild(option);
            });
        }
    } catch (error) {
        console.error('Error cargando coachees:', error);
        select.innerHTML = '<option value="">Error cargando coachees</option>';
    }
    
    // Establecer fecha mínima en ambos formularios (hoy)
    const today = new Date().toISOString().split('T')[0];
    const blockDateInput = document.getElementById('blockDate');
    const directDateInput = document.getElementById('directDate');
    if (blockDateInput) blockDateInput.min = today;
    if (directDateInput) directDateInput.min = today;
}

// Funciones para gestión de solicitudes de sesión
async function acceptSessionRequest(requestId) {
    if (!confirm('¿Confirmar esta sesión?')) return;
    
    try {
        const response = await fetch(`/api/coach/session-requests/${requestId}/accept`, {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' }
        });
        
        const data = await response.json();
        
        if (data.success) {
            showNotification('Sesión confirmada exitosamente', 'success');
            // Recargar solicitudes
            if (window.coachDashboard && typeof window.coachDashboard.loadSessionRequests === 'function') {
                window.coachDashboard.loadSessionRequests();
            }
            // Refrescar calendario
            if (window.coachDashboard && window.coachDashboard.calendar) {
                window.coachDashboard.calendar.refetchEvents();
            }
        } else {
            showNotification(data.error || 'Error al confirmar sesión', 'error');
        }
    } catch (error) {
        console.error('Error accepting session:', error);
        showNotification('Error al confirmar sesión', 'error');
    }
}

async function rejectSessionRequest(requestId) {
    const reason = prompt('Motivo del rechazo (opcional):');
    if (reason === null) return; // Cancelado
    
    try {
        const response = await fetch(`/api/coach/session-requests/${requestId}/reject`, {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({ reason })
        });
        
        const data = await response.json();
        
        if (data.success) {
            showNotification('Solicitud rechazada', 'success');
            // Recargar solicitudes
            if (window.coachDashboard && typeof window.coachDashboard.loadSessionRequests === 'function') {
                window.coachDashboard.loadSessionRequests();
            }
        } else {
            showNotification(data.error || 'Error al rechazar solicitud', 'error');
        }
    } catch (error) {
        console.error('Error rejecting session:', error);
        showNotification('Error al rechazar solicitud', 'error');
    }
}

function openProposeModal(requestId, coacheeName) {
    // Llenar datos del modal
    document.getElementById('originalSessionId').value = requestId;
    document.getElementById('proposeCoacheeName').textContent = coacheeName;
    
    // Abrir modal
    const modal = new bootstrap.Modal(document.getElementById('proposeTimeModal'));
    modal.show();
}

async function submitTimeProposal() {
    const requestId = document.getElementById('originalSessionId').value;
    const date = document.getElementById('proposedDate').value;
    const startTime = document.getElementById('proposedStartTime').value;
    const endTime = document.getElementById('proposedEndTime').value;
    const message = document.getElementById('proposalMessage').value;
    
    if (!date || !startTime || !endTime) {
        showNotification('Por favor completa todos los campos', 'error');
        return;
    }
    
    try {
        const response = await fetch(`/api/coach/session-requests/${requestId}/propose`, {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({
                proposed_date: date,
                proposed_start_time: startTime,
                proposed_end_time: endTime,
                message: message
            })
        });
        
        const data = await response.json();
        
        if (data.success) {
            showNotification('Propuesta enviada al coachee', 'success');
            // Cerrar modal
            const modal = bootstrap.Modal.getInstance(document.getElementById('proposeTimeModal'));
            modal.hide();
            // Limpiar formulario
            document.getElementById('proposeTimeForm').reset();
            // Recargar solicitudes
            if (window.coachDashboard && typeof window.coachDashboard.loadSessionRequests === 'function') {
                window.coachDashboard.loadSessionRequests();
            }
        } else {
            showNotification(data.error || 'Error al enviar propuesta', 'error');
        }
    } catch (error) {
        console.error('Error proposing time:', error);
        showNotification('Error al enviar propuesta', 'error');
    }
}

function proposeAlternative(requestId) {
    // Función legacy - redirigir a openProposeModal
    const coacheeName = 'Coachee'; // Nombre genérico
    openProposeModal(requestId, coacheeName);
}

