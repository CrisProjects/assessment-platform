    if (!editingAssessmentId) {
        showToast('Error: No hay evaluación en modo edición', 'error');
        return;
    }
    
    const data = collectAssessmentData('draft');
    
    try {
        const response = await fetch(`/api/coach/assessments/${editingAssessmentId}/update`, {
            method: 'PUT',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify(data)
        });
        
        const result = await response.json();
        
        if (response.ok) {
            showToast('Borrador actualizado exitosamente', 'success');
            
            // Reset form and reload drafts
            resetAssessmentForm();
            loadDraftAssessments();
            
            // Switch to drafts tab
