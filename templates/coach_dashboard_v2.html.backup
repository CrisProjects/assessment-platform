                // Cargar lista de coachees y llenar los selectores
                await this.populateCoacheeSelectors();
                
                // Cargar contenido asignado
                const response = await fetch('/api/coach/content', {
                    credentials: 'include',
                    headers: {
                        'Accept': 'application/json'
                    }
                });
                const data = await response.json();
                
                if (data.success) {
                    this.assignedContent = data.content || [];
                    this.originalAssignedContent = [...this.assignedContent]; // Guardar copia original
                    
                    // Ocultar loading
                    if (loadingState) loadingState.style.display = 'none';
                    
                    // Renderizar contenido
                    this.renderAssignedContent(this.assignedContent);
                } else {
                    if (loadingState) loadingState.style.display = 'none';
                    if (emptyState) emptyState.style.display = 'block';
                }
            } catch (error) {
                console.error('❌ Error loading assigned content:', error);
                const loadingState = document.getElementById('contentLoadingState');
                const emptyState = document.getElementById('contentEmptyState');
                if (loadingState) loadingState.style.display = 'none';
                if (emptyState) emptyState.style.display = 'block';
            }
