        // Funciones auxiliares
        formatDateShort(dateString) {
            const date = new Date(dateString + (dateString.includes('Z') ? '' : 'Z'));
            return date.toLocaleDateString('es-CL', { 
                timeZone: 'America/Santiago',
                day: '2-digit', 
                month: '2-digit' 
            });
        },
        
        formatDateLong(dateString) {
            const date = new Date(dateString + (dateString.includes('Z') ? '' : 'Z'));
            return date.toLocaleDateString('es-CL', { 
                timeZone: 'America/Santiago',
                year: 'numeric', 
                month: 'long', 
                day: 'numeric' 
            });
        },
        
        formatDimensionName(dimension) {
            const dimensionMap = {
                'dominancia': 'Dominancia',
                'influencia': 'Influencia',
                'estabilidad': 'Estabilidad',
                'cumplimiento': 'Cumplimiento',
                'autoconciencia': 'Autoconciencia',
                'autorregulacion': 'Autorregulación',
                'motivacion': 'Motivación',
                'empatia': 'Empatía',
                'habilidades_sociales': 'Habilidades Sociales',
                'comunicacion': 'Comunicación',
                'colaboracion': 'Colaboración',
                'resolucion_conflictos': 'Resolución de Conflictos',
                'adaptabilidad': 'Adaptabilidad',
                'vision_estrategica': 'Visión Estratégica',
                'toma_decisiones': 'Toma de Decisiones',
                'desarrollo_equipo': 'Desarrollo de Equipo',
                'inspiracion': 'Inspiración',
                'delegacion': 'Delegación'
            };
            return dimensionMap[dimension] || dimension.charAt(0).toUpperCase() + dimension.slice(1).replace(/_/g, ' ');
        },
        
        getDimensionDescription(dimension) {
            const descriptions = {
                'dominancia': 'Orientación a resultados, control y decisión',
                'influencia': 'Capacidad de persuasión y relaciones sociales',
                'estabilidad': 'Paciencia, consistencia y trabajo en equipo',
                'cumplimiento': 'Precisión, calidad y seguimiento de normas',
                'autoconciencia': 'Reconocimiento de emociones propias',
                'autorregulacion': 'Control emocional y adaptación',
                'motivacion': 'Impulso interior y compromiso',
                'empatia': 'Comprensión de emociones ajenas',
                'habilidades_sociales': 'Gestión de relaciones interpersonales',
                'comunicacion': 'Expresión clara y efectiva',
                'colaboracion': 'Trabajo conjunto hacia objetivos comunes',
                'resolucion_conflictos': 'Manejo constructivo de desacuerdos',
                'adaptabilidad': 'Flexibilidad ante cambios',
                'vision_estrategica': 'Pensamiento a largo plazo',
                'toma_decisiones': 'Análisis y elección efectiva',
                'desarrollo_equipo': 'Potenciación del talento',
                'inspiracion': 'Motivación y ejemplo',
                'delegacion': 'Asignación efectiva de responsabilidades'
            };
            return descriptions[dimension] || 'Aspecto evaluado';
        },
        
        getTimeAgo(dateString) {
            const now = new Date();
            const date = new Date(dateString);
            const diffInDays = Math.floor((now - date) / (1000 * 60 * 60 * 24));
            
            if (diffInDays === 0) return 'Hoy';
            if (diffInDays === 1) return 'Ayer';
            if (diffInDays < 7) return `Hace ${diffInDays} días`;
            if (diffInDays < 30) return `Hace ${Math.floor(diffInDays / 7)} semanas`;
            return `Hace ${Math.floor(diffInDays / 30)} meses`;
        },
        
        getScoreColor(percentage) {
            if (percentage >= 80) return 'score-high';
            if (percentage >= 40) return 'score-medium';
            return 'score-low';
        },
        
        generateDimensionsPreview(dimensionalScores) {
            return Object.entries(dimensionalScores)
                .slice(0, 3)
                .map(([dimension, score]) => `
                    <div class="dimension-item">
                        <span class="dimension-name">${dimension}</span>
                        <span class="dimension-score">${score}%</span>
                    </div>
                `).join('');
        },
        
        getLeadershipLevelFromScore(score) {
            if (score >= 40) return 'Alto';
            if (score >= 25) return 'Medio';
            return 'Bajo';
        },
        
        getTeamworkLevelFromScore(score) {
            if (score >= 48) return 'Alto';
            if (score >= 30) return 'Medio';
            return 'Bajo';
        },
        
        getDiscLevelFromScore(score) {
            if (score >= 64) return 'Alto';
            if (score >= 40) return 'Medio';
            return 'Bajo';
        },
        
        getEmotionalIntelligenceLevelFromScore(score) {
            if (score >= 48) return 'Alto';
            if (score >= 30) return 'Medio';
            return 'Bajo';
        },
        
        getAssertiveLevelFromScore(percentage) {
            if (percentage >= 80) return 'Alto';
            if (percentage >= 40) return 'Medio';
            return 'Bajo';
        },
        
